{
    "output": {
        "dir": "css",      // Relative or absolute path (can have ..)
       "structure": "flat"// "flat", "nested", or ["nested", "relative/root/of/nest"]
    },
    "compile": {
        "style": "nested",             // String | Output style: nested/expanded/compact/compressed
        "line-numbers": true,      // Bool   | Emit comments showing original line numbers
